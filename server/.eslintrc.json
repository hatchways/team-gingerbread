{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "prettier/prettier": "error",
    "quotes": [2, "double", { "avoidEscape": true }]
  }
}

{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "prettier/prettier": "error",
    "quotes": [2, "double", { "avoidEscape": true }],
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "object-curly-newline": "warn",
    "consistent-return": "off"
  }
}
